<?xml version="1.0" encoding="utf-8"?>
<com.suncity.dailynotices.islib.widget.SquareRelativeLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="@dimen/dp_109"
        android:layout_height="@dimen/dp_109">

    <com.facebook.drawee.view.SimpleDraweeView
            android:id="@+id/ivImage"
            android:layout_width="@dimen/dp_109"
            android:layout_height="@dimen/dp_109"
            android:layout_centerInParent="true"
            app:viewAspectRatio="1.0"
    />
    <!--app:placeholderImage="@mipmap/ico_empty_holder"
    app:actualImageScaleType="centerCrop"-->

    <ImageView
            android:id="@+id/ivPhotoCheaked"
            android:layout_width="16dp"
            android:layout_height="16dp"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:contentDescription="@null"
            android:src="@drawable/ic_uncheck"/>

</com.suncity.dailynotices.islib.widget.SquareRelativeLayout>